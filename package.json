    "eslint": "~2.2.0",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
